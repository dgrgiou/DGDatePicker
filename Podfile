source 'https://github.com/CocoaPods/Specs.git'
source 'https://bitbucket.org/dgrgiou/dgdatepicker/src/master/DGDatePicker.podspec'

# Uncomment the next line to define a global platform for your project
 platform :ios, '11.0'

target 'DGDatePicker' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  # Pods for DGDatePicker
  pod 'JTAppleCalendar', '~> 7.1.5'
end

target 'Calendar-Client' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for Client
  pod 'DGDatePicker', :path => '/Users/dimitriosgeorgiou/dgrgiou/Apps/iOS/Frameworks/DGDatePicker'

end
